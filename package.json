{
  "name": "react-web-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:develop": "set PORT=4070 && env-cmd -f .env.dev react-scripts start",
    "start:qa": "set PORT=4071 && env-cmd -f .env.qa react-scripts start",
    "start:stage": "set PORT=4072 && env-cmd -f .env.staging react-scripts start",
    "build": "react-scripts build",
    "build:develop": "GENERATE_SOURCEMAP=false env-cmd -f .env.dev react-scripts build",
    "build:stage": "GENERATE_SOURCEMAP=false env-cmd -f .env.staging react-scripts build",
    "build:qa": "GENERATE_SOURCEMAP=false env-cmd -f .env.qa react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false env-cmd -f .env.prod react-scripts build",
    "serve:develop": "GENERATE_SOURCEMAP=false env-cmd -f .env.dev react-scripts build",
    "serve:stage": "GENERATE_SOURCEMAP=false env-cmd -f .env.staging react-scripts build",
    "serve:qa": "GENERATE_SOURCEMAP=false env-cmd -f .env.qa react-scripts build",
    "serve:prod": "GENERATE_SOURCEMAP=false env-cmd -f .env.prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier:write": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md)\" --loglevel error",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.116",
    "@mui/material": "^5.11.5",
    "@mui/x-date-pickers": "^6.0.2",
    "@reduxjs/toolkit": "^1.9.1",
    "antd": "^5.6.2",
    "axios": "^1.3.0",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "lottie-react": "^2.4.0",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dom": "^18.2.0",
    "react-phone-number-input": "^3.2.19",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0",
    "react-scripts": "^5.0.1",
    "react-social-login-buttons": "^3.6.1",
    "react-toastify": "^9.1.2",
    "reactjs-social-login": "^2.6.2",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "rrule": "^2.7.1",
    "sass": "^1.57.1",
    "web-vitals": "^3.1.1",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^29.3.1",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "fs-extra": "^11.1.0",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "json-loader": "0.5.7",
    "prettier": "2.8.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^6.20.0",
    "style-loader": "0.23.1",
    "tailwindcss": "^3.3.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.11.3"
  }
}
